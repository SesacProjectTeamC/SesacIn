<a onclick="moveToDetailBoard('<%= data.bId %>')">
  <div
    style="
      width: 100%;
      height: 100%;
      display: flex;
      justify-content: center;
      align-items: center;
    "
  >
    <div id="cardContainer">
      <board_header><%= cDate%></board_header>
      <div class="title">
        <div class="ellipsis"><%= data.title %></div>
        <div class="viewImg">
          <img
            src="../../static/svg/eye.svg"
            alt="조회수"
            width="24px"
            class="svg"
          />
          <div><%= data.viewCount%></div>
        </div>
      </div>
      <div class="ellipsis"><div class="content">><%= data.content%></div></div>
      <user>
        <%= uName%>
        <div class="like">
          <img
            src="../../static/svg/heart.svg"
            alt="좋아요"
            width="24px"
            class="svg"
          />
          <%= data.likeCount%>
          <img
            src="../../static/svg/message.svg"
            alt="답변개수"
            width="24px"
            class="svg"
          />
          <%= commentCount%>
        </div>
      </user>
    </div>
  </div>
</a>
