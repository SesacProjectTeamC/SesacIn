<a onclick="moveToDetailQuestion('<%= data.qId %>')">
  <div
    style="
      width: 100%;
      height: 100%;
      display: flex;
      justify-content: center;
      align-items: center;
    "
  >
    <div id="cardContainer">
      <ques_header>
        <div><%= cDate%></div>
        <div class="ques_type"><%= data.qType %></div>
      </ques_header>
      <div class="title">
        <div class="ellipsis"><%= data.title%></div>

        <div class="viewImg">
          <img
            src="../../static/svg/eye.svg"
            alt="조회수"
            width="24px"
            class="svg"
            color="gray"
          />
          <%= data.viewCount%>
        </div>
      </div>
      <div class="ellipsis">
        <div class="content"><%= data.content%></div>
      </div>
      <user>
        <%= uName %>
        <div class="like">
          <img
            src="../../static/svg/heart.svg"
            alt="좋아요"
            width="24px"
            class="svg"
          />
          <%= data.likeCount%>
          <img
            src="../../static/svg/message.svg"
            alt="좋아요"
            width="24px"
            class="svg"
          />
          <%= commentCount %>
        </div>
      </user>
    </div>
  </div>
</a>
